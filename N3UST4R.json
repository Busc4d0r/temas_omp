{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "final_space": true,
    "version": 3,
    "console_title_template": "{{ .Folder }}",
    "transient_prompt": {
        "background": "transparent",
        "foreground": "#D6DEEB",
        "template": "\ue285 "
    },
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "os",
                    "style": "diamond",
                    "leading_diamond": "\ue0b2",
                    "trailing_diamond": "<transparent,#0F7C11>\ue0b2</>",
                    "foreground": "#D6DEEB",
                    "background": "#0F7C11",
                    "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
                    "properties": {
                        "windows": "\ue62a ",
                        "ubuntu": "\uf31b "
                    }
                },
                {
                    "type": "shell",
                    "style": "diamond",
                    "leading_diamond": "\ue0b2",
                    "trailing_diamond": "<transparent,#3A3A3A>\ue0b2</>",
                    "foreground": "#D6DEEB",
                    "background": "#3A3A3A",
                    "properties": {
                        "mapped_shell_names": {
                            "pwsh": "\uebc7",
                            "bash": "\ue760"
                        }
                    }
                },
                {
                    "type": "session",
                    "style": "diamond",
                    "leading_diamond": "\ue0b2",
                    "trailing_diamond": "\ue0b0",
                    "foreground": "#D6DEEB",
                    "background": "#0F7C11",
                    "template": " {{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }} "
                },
                {
                    "type": "executiontime",
                    "style": "diamond",
                    "leading_diamond": "<transparent,#3A3A3A>\ue0b0</>",
                    "trailing_diamond": "\ue0b0",
                    "foreground": "#D6DEEB",
                    "background": "#3A3A3A",
                    "template": " <#fefefe>\ue641</> {{ .FormattedMs }} ",
                    "properties": {
                        "threshold": 500,
                        "style": "austin",
                        "always_enabled": true
                    }
                },
                {
                    "type": "git",
                    "style": "diamond",
                    "leading_diamond": "<transparent,#0F7C11>\ue0b0</>",
                    "trailing_diamond": "\ue0b0",
                    "foreground": "#D6DEEB",
                    "background": "#0F7C11",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#0F7C11{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#0F7C11{{ end }}",
                        "{{ if gt .Ahead 0 }}#0F7C11{{ end }}",
                        "{{ if gt .Behind 0 }}#0F7C11{{ end }}"
                    ],
                    "template": " {{ .HEAD }}{{if .BranchStatus }}{{ .BranchStatus }}{{ end }} {{ if .Working.Changed }} \uF044 {{ .Working.String }}{{ end }} {{ if and (.Staging.Changed) (.Working.Changed) }}|{{ end }} {{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }} ",
                    "properties": {
                        "fetch_status": true,
                        "fetch_upstream_icon": true,
                        "untracked_modes": {
                            "/Users/user/Projects/oh-my-posh/": "no"
                        },
                        "source": "cli",
                        "mapped_branches": {
                            "feat/*": "üöÄ ",
                            "bug/*": "üêõ "
                        }
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "newline": true,
            "alignment": "left",
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "template": "\u256d\u2500"
                },
                {
                    "type": "time",
                    "style": "plain",
                    "foreground": "#D6DEEB",
                    "properties": {
                        "time_format": "15:04"
                    }
                },
                {
                    "type": "root",
                    "style": "plain",
                    "template": " \uF0E7 "
                },
                {
                    "type": "path",
                    "style": "plain",
                    "foreground": "#D6DEEB",
                    "template": "{{ .Path }}",
                    "properties": {
                        "home_icon": "\ued3e ",
                        "folder_separator_icon": " \uf061 ",
                        "folder_icon": "\uf07b "
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "status",
                    "style": "plain",
                    "template": "\u2570\u2500 ",
                    "properties": {
                        "always_enabled": true
                    }
                }
            ]
        }
    ]
}